{
    "name": "unity-web",
    "version": "0.0.1",
    "description": "Unity But Fucking Web",
    "main": "server/index.js",
    "scripts": {
        "start": "node server/index.js",
        "build": "webpack --mode production",
        "watch": "webpack --watch --mode development",
        "dev": "concurrently \"npm:start\" \"npm:watch\" \"npm:open-browser\"",
        "open-browser": "sleep 5 && open-cli http://localhost:3000"
    },
